	//jsonp回调
	window.tvStationBack = function(data){
		if(data.response && data.response.code===0){
			var info = data.data;
			var list = info.lives;
			if(list && list.length>0){	
				var len = list.length;
				var htmlTxt = C.buildHtml(list,len);
				$('[data-node="list"]','#zhibotai').html(htmlTxt);
				C.buildBeaus();//获取对象数据
				if(cookie.getItem('dc_tv_closeAuto')=='1'){
					$('[node-type="autoPlay"]','#zhibotai').addClass('close');
				}
				//初始化切换按钮
				$(C.beaus['slide'][0]).hide();
				if(list.length<=C.perNum){
					$(C.beaus['slide'][1]).hide();
				}
				//有直播则创建播放框
				if(C.curPos!=null){
					C.buildPlayer(list[C.curPos]);
					zhiboT_nodeReplace && $('#'+zhiboT_nodeReplace).hide();
					$('#zhibotai').show();
					bind();
				}else{
					$('[data-node="play"]','#zhibotai').hide();
					$('#zhibotai').hide();
				}								
				
			}
		}
	}
	var C={
		curPos:null,//当前播放视频序号
		pureData:[],//存放需要的数据，包括直播id，序号等
		beaus:{},//对象集合
		curScreen:0,
		count:0,//总屏数
		perNum:3,//每屏条数
		stringToJson:function(str){
			if(str){
				var arr = str.split('&');
				var data ={};
				for(var i=0;i<arr.length;i++){
					var temp = arr[i].split('=');
					data[temp[0]] = temp[1];
				}
				return data;
			}
		},
		buildBeaus:function(){//获取需要的对象
			C.beaus['iframe'] = $('[data-node="liveBox"] iframe','#zhibotai');
			C.beaus['go'] = $('[data-node="go"]','#zhibotai');
			C.beaus['playBtn'] = $('[data-node="playBtn"]','#zhibotai');
			C.beaus['liveBox'] = $('[data-node="liveBox"]','#zhibotai');
			C.beaus['list'] =$('[data-node="list"]','#zhibotai');
			C.beaus['item'] = $('[data-node="item"]','#zhibotai');
			C.beaus['slide'] = $('[data-node="slide"]','#zhibotai');
			C.count = Math.ceil(C.beaus['item'].length/C.perNum);
		},
		checkLocal:function(){//检查距离第一次直播出现是否超过24h
			var curTime = (new Date()).getTime();
			var lastTime = localStorage.getItem('dc_tv_time');	 
			if(localStorage.getItem('dc_tv_play') == 1 && curTime - lastTime >= 24*60 * 60 * 1000){
		    	localStorage.removeItem('dc_tv_play');
		        localStorage.removeItem('dc_tv_play')
		    }
		},
		buildHtml:function(list,len){
			var allH=[];
			var sta = false;//开关
			for(var i=0;i<len;i++){
				if(!sta && list[i].playing_status==1 && list[i].stream_id){
					C.curPos = i;//当前播放视频序号
					sta = true;
				}							
				//playing_status: 1直播中 2已结束  3未开始
				var mainH=[];
				var match = list[i].match;
				if(match && match.homeName && match.awayName){//对阵赛
					mainH=['<div class="match clearfix">',
								'<span class="col1"><span>',match.homeName,'</span>',list[i].playing_status==1?'<br><em>'+match.homeGoal+'</em>':'','</span>',
								list[i].playing_status==1?'<span class="col2 playing">正在直播</span>':'<span class="col2"><span class="no">未开始</span><br/><span>'+list[i].start_time.substring(5,16)+'</span></span>',
								'<span class="col3"><span>',match.awayName,'</span>',list[i].playing_status==1?'<br><em>'+match.awayGoal+'</em>':'','</span>',
							'</div>'].join('');
				}else{//非对阵赛
					mainH=['<div class="info">',
								'<p>',list[i].title,'</p>',
								list[i].playing_status==1?'<span class="playing">正在直播</span>':'<span>'+list[i].start_time.substring(5,16)+'</span>',
							'</div>'].join('');
				}							
				var item=['<li data-node="item"',list[i].playing_status==3?' class="noStart"': ' data-action="item" data-data="pos='+i+'"','>',
						list[i].playing_status==3?'<a class="linkZB" target="_blank" href="'+list[i].shared.pc_url+'"></a>':'',

						'<strong>',list[i].catalog_name,'</strong>',
						mainH,
				'</li>'].join('');							
				allH.push(item);
				//存储数据{序号、直播ID、播放地址}
				C.pureData.push({'pos':i,'id':list[i].stream_id,'url':list[i].shared.pc_url});		
			}
			
			return allH.join('');
		},
		updataLive:function(pos){
			var src = 'http://v.qq.com/iframe/live_player.html?cnlid='+C.pureData[pos].id+'&width=300&height=212';
			C.beaus['iframe'].attr('src',src);
			C.beaus['go'].attr('href',C.pureData[pos].url);
			C.beaus['liveBox'].show();
			C.beaus['playBtn'].hide();	
			//更新页面显示状态
			C.beaus['item'].removeClass('cur');
			$(C.beaus['item'][pos]).addClass('cur');
			ExposureBoss(1604, 'szbtbfk', 'zhibotai');
		},
		buildPlayer:function(item){
			//aboutVideo_v: 1相关视频开启自动播放  0关闭自动播放
			//var aboutVideo_v = cookie.getItem('aboutVideo_v');
			var dc_tv_play = cookie.getItem('dc_tv_play');
			var dc_tv_closeAuto = cookie.getItem('dc_tv_closeAuto');
			//自动播放关闭或24小时内自动播放过
			if(dc_tv_closeAuto=='1' ||dc_tv_play == '1'){//已经播放过直播，显示图片				
				//获取直播的封面图
				var picUrl = item.images && item.images['300x168']?item.images['300x168'].url:null;
				picUrl && C.beaus['playBtn'].attr('src',picUrl);
				//显示直播封面图
				C.beaus['playBtn'].show();
				C.beaus['liveBox'].hide();
				//更新页面上当前状态
				$(C.beaus['item'][C.curPos]).addClass('cur');
			}else{//开启自动播放&&未播放过直播，显示直播								//底层原有相关视频静音
				isRelvideoMute =true;
				C.updataLive(C.curPos);
				//存放播放信息
				cookie.setItem('dc_tv_play','1',24*60,'qq.com');
				//localStorage.setItem('dc_tv_play', 1);//是否播放
				//localStorage.setItem('dc_tv_time', (new Date).getTime());//播放时间
			}
		}
	};
	var bindFun={
		'itemClk':function(e){
			if($(this).hasClass('cur')){return;}
			var obj =C.stringToJson($(this).attr('data-data'));
			var pos = parseInt(obj.pos);
			C.updataLive(pos);
			C.curPos = pos;
			registerZone2({bossZone:'zbtlisttitle',url:''},1);
		},
		'playBtn':function(e){
			C.updataLive(C.curPos);
			registerZone2({bossZone:'zbtbficon',url:''},1);	
		},
		'slide':function(e){
			var obj = C.stringToJson($(this).attr('data-data'));
			var idx = parseInt(obj.idx);					
			that = this;
			if((idx>0 &&C.curScreen>0) || (idx<0 && C.curScreen<C.count-1)){							
				C.curScreen -= idx;
				C.beaus['list'].animate({'marginTop':'+='+idx*56*C.perNum+'px'},1000);
				C.beaus['slide'].show();
				if(C.curScreen<=0 || C.curScreen>=C.count-1){
					$(that).hide();
				}
			}
			var sop = idx>0?'zbtlistprev':'zbtlistnext';
			registerZone2({bossZone:sop,url:''},1);						
		},
		'autoPlay':function(e){						 
			if(cookie.getItem('dc_tv_closeAuto') == '1'){
				$(this).removeClass('close');
		    		cookie.removeItem('dc_tv_closeAuto','qq.com');//开启自动播放
		    		registerZone2({bossZone:'zbtplayopen',url:''},1);
		    }else{
		    	$(this).addClass('close');
		    	cookie.setItem('dc_tv_closeAuto','1',30*24*60,'qq.com');//关闭自动播放	
	    		registerZone2({bossZone:'zbtplayclose',url:''},1);	
		    }
		},
		'enter':function(e){
			C.beaus['go'].fadeIn();
			ExposureBoss(1604, 'szbtbfkenter', 'zhibotai');
		},
		'leave':function(e){
			C.beaus['go'].fadeOut();			
		}
	};
	var bind= function(){
		$('[data-action="item"]','#zhibotai').bind('click',bindFun['itemClk']);
		C.beaus['playBtn'].bind('click',bindFun['playBtn']);
		C.beaus['slide'].bind('click',bindFun['slide']);
		$('[node-type="autoPlay"]','#zhibotai').bind('click',bindFun['autoPlay']);
		C.beaus['liveBox'].bind('mouseenter',bindFun['enter']);
		C.beaus['liveBox'].bind('mouseleave',bindFun['leave']);
	};
	var init = function(){
		var obj = document.getElementById('zhibotai');
		if(!zhiboT_nodeId || !obj){return;}
		//C.checkLocal();
		
		//获取直播台数据
		var urlTxt='http://data.v.qq.com/live/v2/api/live/nodelives?scope=news_detail&reqsrc=news_detail';
		$.ajax({
			url:urlTxt,
			data:{'node_id':zhiboT_nodeId,'number':12},
			dataType:'jsonp',
			jsonpCallback:'tvStationBack'
		});
	};
	init();
})();
</script><!--[if !IE]>|xGv00|b373a50a8b5bb670f3c634f28cd8b157<![endif]-->




