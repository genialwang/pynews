	this.y = qq.getY(obj);
	obj.parentNode.style.height = qq.height(obj) + 'px';
	this.scrollFun(obj);	
}

Widget.scroll.prototype = {
	scrollFun: function(obj){
		var foot = qq.GC(qq.G('Wrap'), '.foot-Article-QQ')[0];
		var _this = this, DH = 0, WH = 0, H = qq.height(foot) + qq.height(obj), SH = 0, MH = 0;
		
		qq.EA(window, 'scroll', function(){
			var t = qq.scrollY(), MH = qq.height(qq.G('Main-Article-QQ')), WH = qq.windowHeight();
			
			DH = document.body.clientHeight;
			SH = DH - WH;

			if(_this.y + qq.height(obj) > MH) {obj.style.cssText = 'position:static';};
			if(t >= _this.y){
				if(t >= DH - WH) return;
				if(t > DH - H ){
					obj.style.position = 'absolute';
					obj.style.top = MH - qq.height(obj) + 'px';//Main-Article-QQ
				}else{
					obj.style.cssText = 'position:fixed; top:0';
				}
				if(ie6){
					obj.style.position = 'absolute';
					ie6Fixed(obj, 0);
				}
